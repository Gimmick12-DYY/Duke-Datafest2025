   "source": [
    "from graphviz import Source\n",
    "# Load and render the DOT file\n",
    "with open(\"tree.dot\") as f:\n",
    "    dot_graph = f.read()\n",
    "\n",
    "graph = Source(dot_graph)\n",
    "graph.format = \"svg\"\n",
    "graph.render(\"tree\", cleanup=True)  \n"
   ]
  }
